# Place in root directory - single source of truth of configuration
DEBUG=True

# Model name for LiteLLM (azure/<deployment-name> or gpt-4o for OpenAI direct)
MODEL_NAME=azure/gpt-5.2

AZURE_OPENAI_ENDPOINT=https://xxxxxxxxxxxxxx
AZURE_OPENAI_API_VERSION=2025-03-01-preview
AZURE_OPENAI_INFERENCING_DEPLOYMENT=gpt-5.2
AZURE_OPENAI_CLASSIFIER_DEPLOYMENT=gpt-4.1-mini

# Telemetry
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=xxxxxxxxxxxxxxxxxxxxxxx

# App environment label
APP_ENVIRONMENT=dev

# Development Feature
# Auto-open browser on startup (set to true for local development) for backend Swagger UI
OPEN_BROWSER=true

# Hub configuration settings
AZURE_SUBSCRIPTION_ID=
AZURE_RESOURCE_GROUP=
AZURE_AI_PROJECT_NAME=